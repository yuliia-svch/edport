<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GradeLimit</fullName>
    <active>true</active>
    <errorConditionFormula>Grade__c  &gt; 100</errorConditionFormula>
    <errorDisplayField>Grade__c</errorDisplayField>
    <errorMessage>Grade cannot be more than 100</errorMessage>
</ValidationRule>
